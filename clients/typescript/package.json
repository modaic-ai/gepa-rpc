{
  "name": "gepa-rpc",
  "version": "0.2.1",
  "description": "Run GEPA on your favorite non-python frameworks.",
  "author": "Tyrin Todd <tyrin@modaic.dev>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/modaic-ai/gepa-rpc"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./ai-sdk": {
      "types": "./dist/ai-sdk/prompt.d.ts",
      "default": "./dist/ai-sdk/prompt.js"
    }
  },
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@ai-sdk/openai": "3.0.0",
    "ai": "^6.0.27",
    "bumpp": "^10.4.0",
    "fastify": "^5.6.2",
    "nanoid": "^5.1.6",
    "yaml": "^2.8.2",
    "zod": "3"
  },
  "scripts": {
    "build": "tsc && bun build ./src/index.ts ./src/ai-sdk/prompt.ts --outdir ./dist --target node"
  }
}
